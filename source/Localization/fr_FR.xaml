    <!-- SuccessStorySearch -->

    <sys:String x:Key="LOCSuccessStorySearchDescription">Recherche sur les données de SuccessStory</sys:String>
    <sys:String x:Key="LOCSuccessStorySearchHint" xml:space="preserve">Paramètres :
    -percent		Pourcentage de progression (ex:-percent=50 / -percent=&lt;50 / -percent=&gt;50 / -percent=10&lt;&gt;50)
    -time		Temps requis pour terminer le jeu à 100% (ex:-time=5h / -time=&lt;50min / -time=&gt;50s / -time=10min&lt;&gt;5h)
    -np		Vous n'avez pas joué à ce jeu
    -fav		Le jeu fait partie des favoris
    -stores={n}	Filtrer par nom de bibliothèque (exemple : -stores=steam / -stores=steam,epic)
    -status={n}	Filtrer par nom de statut (exemple : -status=played / -status=played,playing)</sys:String>


